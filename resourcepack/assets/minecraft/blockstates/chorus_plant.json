{
  "multipart": [
    {
      "apply": {
        "model": "minecraft:block/chorus_plant"
      }
    },
	{
      "when": {
        "up": "false",
		"north": "false",
		"south": "false",
		"east": "false",
		"west": "false"
      },
      "apply": {
        "model": "minecraft:block/chorus_plant_inside_face"
      }
    },
	{
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "minecraft:block/chorus_plant_branch"
