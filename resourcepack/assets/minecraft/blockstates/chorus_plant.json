      "apply": {
        "model": "minecraft:block/chorus_plant_branch",
        "y": 270,
        "uvlock": false
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "minecraft:block/chorus_plant_branch_up"
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "minecraft:block/chorus_plant_branch_down"
